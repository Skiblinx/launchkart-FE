# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:8000

# Payment Gateway Configuration - Razorpay (India)
REACT_APP_RAZORPAY_KEY_ID=your-razorpay-key-id

# Payment Gateway Configuration - Stripe (International)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Google Analytics (optional)
REACT_APP_GA_TRACKING_ID=your-google-analytics-id

# Environment
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_PAYMENT=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_KYC=true